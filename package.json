{
  "name": "VOX",
  "productName": "VOX",
  "version": "0.0.1",
  "engines": {
    "node": "4.x"
  },
  "main": "main.js",
  "scripts": {
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "lint": "eslint .",
    "lint-only-change": "eslint",
    "hot-server": "node -r babel-register server.js",
    "build": "cross-env NODE_ENV=production node -r babel-register ./node_modules/.bin/ --config webpack.config.prod.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main",
    "precommit": "./scripts/pre-commit.sh",
    "prepush": "npm test"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron",
    "eslint": "./node_modules/.bin/eslint",
    "karma": "./node_modules/.bin/karma"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "glob": "7.0.3",
    "immutable": "3.7.6",
    "intl-messageformat": "1.3.0",
    "keymirror": "0.1.1",
    "lodash": "4.11.1",
    "moment": "2.12.0",
    "moment-duration-format": "1.3.0",
    "musicmetadata": "2.0.2",
    "node-localstorage": "1.3.0",
    "node-uuid": "1.4.7",
    "react": "15.0.1",
    "react-addons-pure-render-mixin": "15.0.1",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.0.1",
    "react-intl": "2.1.2",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-persist": "3.1.1",
    "redux-thunk": "2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.3",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "chai-immutable": "1.5.4",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "electron-debug": "0.6.0",
    "electron-packager": "6.0.2",
    "electron-prebuilt": "1.0.2",
    "electron-rebuild": "1.1.3",
    "eslint": "2.9.0",
    "eslint-plugin-react": "5.1.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "husky": "0.11.1",
    "karma": "0.13.22",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chai-immutable": "1.0.2",
    "karma-electron": "3.2.1",
    "karma-intl-shim": "1.0.3",
    "karma-mocha": "1.0.1",
    "karma-sinon-chai": "1.2.0",
    "karma-webpack": "1.7.0",
    "lolex": "1.4.0",
    "mocha": "2.4.5",
    "postcss-loader": "0.8.2",
    "react-hot-loader": "1.3.0",
    "redux-logger": "2.6.1",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.0",
    "stylus-loader": "1.5.1",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
